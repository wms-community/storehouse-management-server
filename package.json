{
  "name": "storehouse-management-server",
  "version": "dev",
  "description": "A PHP system for efficient management of your warehouse.",
  "repository": {
    "type": "git",
    "url": "https://github.com/wms-community/storehouse-management-server"
  },
  "author": "SANYIMOE",
